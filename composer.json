{
  "name": "exbico/monolog-db-bundle",
  "description": "Symfony MonologDb Bundle",
  "keywords": ["symfony", "monolog", "bundle"],
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Contributors",
      "homepage": "https://github.com/exbico/monolog-db-bundle/contributors"
    }
  ],
  "require": {
    "php": "^8.0",
    "doctrine/dbal": "^2.9|^3.0",
    "exbico/monolog-db-handler": "^1.0|^2.0",
    "symfony/console": "^5.0|^6.0",
    "symfony/framework-bundle": "^5.0|^6.0",
    "symfony/monolog-bundle": "^3.7"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "slevomat/coding-standard": "^8.12",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "Exbico\\MonologDbBundle\\": "src/"
    }
  }
}
